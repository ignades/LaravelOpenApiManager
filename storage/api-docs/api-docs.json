{
    "openapi": "3.0.0",
    "info": {
        "title": "User Management API",
        "description": "This is an example API for users management",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/"
        }
    ],
    "paths": {
        "/api/products": {
            "get": {
                "description": "LIST Product method get",
                "responses": {
                    "200": {
                        "description": "Successfully response!",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/getProducts"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "getProducts": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "price": {
                        "type": "number"
                    },
                    "stock": {
                        "type": "integer"
                    },
                    "created_at": {
                        "type": "string"
                    },
                    "updated_at": {
                        "type": "string"
                    }
                }
            }
        }
    }
}